{
	"kestrel": {
		"endpoints": {
			"http": {
				// HTTP/1.1 endpoint for health checks
				"url": "http://*:5000",
				"protocols": "Http1"
			},
			"grpc": {
				// HTTP/2 endpoint for gRPC services
				"url": "http://*:5002",
				"protocols": "Http2"
			}
		}
	},

	"logging": {
		"logLevel": {
			"default": "Debug",
			"system": "Information",
			"grpc": "Information",
			"microsoft": "Information"
		}
	}
}
